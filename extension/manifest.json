{
    "name": "Token signing Mass",
    "version": "0.0.6",
    "minimum_chrome_version": "40.0",
    "manifest_version": 2,
    "description": "Use your eID smart card on the web with multiple file signing capabilities",
    "icons": {
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "content_scripts": [{
        "matches": ["*://*/*", "file:///*"],
        "js": ["content-mass.js"],
        "run_at": "document_end",
        "all_frames": true
    }],
    "background": {
        "scripts": ["background-mass.js"]
    },
    "permissions": ["nativeMessaging"],
    "applications": {
        "gecko": {
            "id": "{443830f0-1fff-4f9a-aa1e-444bafbc7319}"
        }
    }
}
